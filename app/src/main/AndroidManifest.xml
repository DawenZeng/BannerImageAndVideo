        </activity>
        <activity android:name=".BannerActivity" />
        <activity android:name=".TwoActivity" />
        <activity android:name=".ThreeActivity" />
        <activity android:name=".FourActivity" />
        <activity
            android:name=".FiveActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SixActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SevenActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".qsvideo.RecyVideoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".EightActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".SharePreferenceSaveListActivity"></activity>
    </application>

</manifest>